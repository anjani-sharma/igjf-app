{
  "version": 2,
  "public": true,
  "outputDirectory": "dist-merged",
  "buildCommand": null,
  "installCommand": null,
  "devCommand": null,
  "framework": null,
  "env": {
    "EXPO_PUBLIC_API_URL": "https://igjf-app.onrender.com/api"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}